{
  "name": "release-preparer",
  "version": "0.2.4",
  "author": "Jake Ginnivan <jake@ginnivan.net>",
  "license": "MIT",
  "bin": {
    "release-preparer": "index.js"
  },
  "publishConfig": {
    "registry": "https://nexus.swmdigital.io/repository/js-private"
  },
  "scripts": {
    "build": "tsc && rollup src/git-release-notes.js --format cjs --output dist/release-preparer.js --environment INCLUDE_DEPS,BUILD:production",
    "watch": "tsc -w",
    "test": "jest"
  },
  "devDependencies": {
    "@types/commander": "^2.9.0",
    "@types/fs-extra": "^3.0.2",
    "@types/glob": "^5.0.30",
    "@types/jest": "^19.2.3",
    "@types/marked": "^0.0.28",
    "@types/pino": "^3.0.1",
    "commander": "^2.9.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "jest": "^20.0.4",
    "marked": "^0.3.6",
    "pino": "^4.7.1",
    "ts-jest": "^20.0.4",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
